local Players = game:GetService("Players")
local localName = Players.LocalPlayer.Name

-- รวมชื่อทั้งหมดไว้ในตารางเดียว
local allowedNames = {}

-- รวม HelperNameList
for _, name in ipairs(_G.Settings_V4["HelperNameList"]) do
    allowedNames[name] = true
end

-- รวม V4FarmList
for _, name in ipairs(_G.Settings_V4["V4FarmList"]) do
    allowedNames[name] = true
end

-- เช็คว่าชื่อ LocalPlayer อยู่ในตารางหรือไม่
if not allowedNames[localName] then
    return -- ออกจาก script เลย ไม่ทำงานต่อ
end

--repeat wait() until game.Players.LocalPlayer:FindFirstChild("PlayerGui") and game.Players.LocalPlayer.PlayerGui:FindFirstChild("Main")
time_execute = 0
while true do 
    --wait(20)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/xshiba/MasterPClient/main/Loader.lua"))()
    time_execute = time_execute + 1
    if time_execute >= 2 then
        break
    end
end
    
